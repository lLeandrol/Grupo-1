<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prueba10">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Grupo11">
        <activity
            android:name="com.example.prueba10.MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.Grupo11.NoActionBar"></activity>
        <activity android:name="com.example.prueba10.RegistroActivity" />
        <activity android:name="com.example.prueba10.HomeActivity" />
        <activity android:name="com.example.prueba10.TerminosActivity" />
        <activity android:name="com.example.prueba10.LoginActivity" android:theme="@style/Theme.Grupo11.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.prueba10.MainActivity" />
    </application>

</manifest>